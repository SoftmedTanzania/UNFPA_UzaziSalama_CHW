                        android:padding="4dp"
                        android:id="@+id/textPhone"
                        android:text="Mobile #"
                        android:background="@drawable/cell_shape_header"
                        android:layout_height="wrap_content"
                        />
                    <TextView
                        android:layout_weight="0.1"
                        android:padding="4dp"
                        android:id="@+id/risk"
                        android:text="Risk referral_status"
                        android:background="@drawable/cell_shape_header"
