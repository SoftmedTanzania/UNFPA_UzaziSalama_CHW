                    <TextView
                        android:layout_weight="0.1"
                        android:padding="4dp"
                        android:id="@+id/App4"
                        android:text="App4"
                        android:background="@drawable/cell_shape_header"
                        android:layout_height="wrap_content"
                        />
                    <TextView
                        android:layout_weight="0.1"
                        android:padding="4dp"
                        android:id="@+id/edd"
                        android:text="EDD"
                        android:background="@drawable/cell_shape_header"
                        android:layout_height="wrap_content"
